{
    "name": "bkwld/laravel-pug",
    "description": "Pug view adapter for Laravel",
    "type": "library",
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "4 - 5",
        "illuminate/view": "4 - 5",
        "pug-php/pug": "1 - 2"
    },
    "autoload": {
        "psr-4": {
            "Bkwld\\LaravelPug\\": "src/"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Robert Reinhard",
            "email": "info@bukwild.com"
        }
    ],
    "scripts": {
        "post-update-cmd": "Bkwld\\LaravelPug\\Install::publishVendor",
        "post-package-install": "Bkwld\\LaravelPug\\Install::publishVendor",
        "post-install-cmd": "Bkwld\\LaravelPug\\Install::publishVendor",
        "post-autoload-dump": "Bkwld\\LaravelPug\\Install::publishVendor",
        "post-create-project-cmd": "Bkwld\\LaravelPug\\Install::publishVendor"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Bkwld\\LaravelPug\\ServiceProvider"
            ]
        }
    }
}
